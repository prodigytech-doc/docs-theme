{
  "name": "metaapp-prodigytech-doc-theme",
  "version": "1.1.0",
  "description": "Vite & Vue powered static site generator",
  "types": "./theme.d.ts",
  "exports": {
    ".": {
      "types": "./theme.d.ts",
      "default": "./src/client/theme-default/index.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "types",
    "tsconfig.json",
    "client.d.ts",
    "theme.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuejs/vitepress.git"
  },
  "keywords": [
    "vite",
    "vue",
    "vitepress"
  ],
  "author": "Evan You",
  "license": "MIT",
  "homepage": "https://github.com/vuejs/vitepress/tree/main/#readme",
  "bugs": {
    "url": "https://github.com/vuejs/vitepress/issues"
  },
  "scripts": {
    "build": "run-s build-prepare build-client",
    "build-client": "vue-tsc --noEmit -p src/client && tsc -p src/client && node scripts/copyClient",
    "build-prepare": "rimraf dist && node scripts/copyShared",
    "format": "prettier --check --write .",
    "format-fail": "prettier --check .",
    "check": "run-s format-fail build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "node scripts/release.mjs"
  },
  "dependencies": {
